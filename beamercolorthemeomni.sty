\definecolor{omni-background}{RGB}{25,22,34}
\definecolor{omni-foreground}{RGB}{225,225,230}
\definecolor{omni-cyan}{RGB}{68,121,130}
\definecolor{omni-selection}{RGB}{65,65,77}
\definecolor{omni-comment}{RGB}{72,60,103}
\definecolor{omni-orange}{RGB}{232,158,100}

\setbeamercolor{background canvas}{bg=omni-foreground}
\setbeamercolor{normal text}{fg=omni-background}

\setbeamercolor{frametitle}{bg=omni-background}
\setbeamercolor{progress bar}{fg=omni-cyan}
\setbeamercolor{title separator}{fg=omni-cyan}
\setbeamercolor{progress bar in head/foot}{fg=omni-cyan}
\setbeamercolor{progress bar in section page}{fg=omni-cyan}
\setbeamercolor{footline}{bg=omni-background, fg=omni-foreground}

